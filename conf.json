{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "theme": "spacelab"
    },
    "opts": {
        "template": "./node_modules/ink-docstrap/template",
        "destination": "./doc/",
        "recurse": true,
        "readme": "./README.md"
    }
}